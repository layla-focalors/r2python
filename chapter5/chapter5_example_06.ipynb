   "source": [
    "# 연습문제 06 , p183\n",
    "from scipy.stats import poisson\n",
    "\n",
    "mu = 3 # 평균 3건의 교통사고 가정\n",
    "k = 4  # 4건의 교통사고가 발생할 확률\n",
    "u = 2  # 2건의 교통사고가 발생할 확률\n",
    "\n",
    "result = poisson.pmf(k, mu)\n",
    "print(f\"정확히 4건의 교통사고가 일어날 확률 : {result:.3f}\")\n",
    "result = poisson.cdf(u, mu)\n",
    "print(f\"3건 미만의 교통사고가 일어날 확률 : {result:.3f}\")"
   ]
  }
 ],
