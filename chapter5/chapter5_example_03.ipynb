   "source": [
    "# 연습문제 07 , p182\n",
    "from scipy.stats import hypergeom\n",
    "\n",
    "M = 25 # 전체 동물 수\n",
    "n = 10 # 전체 동물 중 꼬리표가 있는 동물 수\n",
    "N = 15 # 뽑은 동물 수\n",
    "k = 5  # 뽑은 동물 중 꼬리표가 있는 동물 수\n",
    "\n",
    "result = hypergeom.pmf(k, M, n, N)\n",
    "print(f\"생포한 동물 5마리에 꼬리표가 있을 확률 : {result:.4f}\")"
   ]
  }
