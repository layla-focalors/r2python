   "source": [
    "# 통계적 추정\n",
    "# 예제(7.2), p220\n",
    "from math import sqrt\n",
    "from scipy.stats import norm\n",
    "\n",
    "n = 225\n",
    "x = 18\n",
    "p_hat = x / n\n",
    "z = norm.ppf(0.975)\n",
    "se = sqrt(p_hat * (1 - p_hat) / n)\n",
    "lower = p_hat - z * se\n",
    "upper = p_hat + z * se\n",
    "\n",
    "print(f\"95% 신뢰구간 : {round((lower), 3)}, {round((upper), 3)}\")\n",
    "\n"
   ]
  }
