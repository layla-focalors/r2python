   "source": [
    "# 예제(7.11), p229\n",
    "from math import sqrt\n",
    "\n",
    "n     = 10      # 표본의 크기\n",
    "x1    = 27.2    # 표본평균\n",
    "s     = 1.8     # 표본표준편차\n",
    "t     = 2.262   # t분포표에서 검정유의수준 0.05\n",
    "\n",
    "lower = x1 - (t * s / sqrt(n))\n",
    "upper = x1 + (t * (s / sqrt(n)))\n",
    "\n",
    "print(f\"95% 신뢰구간 추정 : {round((lower), 1)} < µ < {round((upper), 1)}\")"
   ]
  }
