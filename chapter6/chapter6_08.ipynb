   "source": [
    "# 예제(6.14), p205\n",
    "from scipy.stats import chi2\n",
    "\n",
    "sample = [1.9, 2.4, 3.0, 3.5, 4.2]\n",
    "n = len(sample)\n",
    "mu = 3\n",
    "sigma = 1\n",
    "\n",
    "x2 = sum(((x - mu) / sigma) ** 2 for x in sample)\n",
    "p = 1 - chi2.cdf(x2, df=n-1)\n",
    "\n",
    "print(f\"x²의 값 : {round((x2), 3)}\")\n",
    "print(f\"P(x² > x₀²) : {round((p), 3)}\")\n"
   ]
  }
