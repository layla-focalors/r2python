   "source": [
    "# 연습문제 34 p216\n",
    "from scipy.stats import f\n",
    "\n",
    "n1 = 8\n",
    "n2 = 12\n",
    "f_ratio = 4.5\n",
    "\n",
    "dof1 = n1 - 1\n",
    "dof2 = n2 - 1\n",
    "\n",
    "p_value = 1 - f.cdf(f_ratio, dof1, dof2)\n",
    "print(f\"표본분산비가 4.5 이상일 확률 : {round((p_value), 2)}\")"
   ]
  }
